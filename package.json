{
  "name": "@agent-ly/nestjs-agenda",
  "version": "1.1.2",
  "description": "An opioninated agenda module for Nestjs",
  "repository": "https://github.com/agent-ly/nestjs-agenda.git",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "nest build",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.4.7",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.12.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.11 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.10.11 || ^7.0.0 || ^8.0.0",
    "agenda": "^4.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
